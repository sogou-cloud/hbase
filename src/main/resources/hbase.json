	},
    	{
		"objectName" : "hadoop:service=HBase,name=RPCStatistics-60020",
		"resultAlias": "rs.rpc",
		"attributes" : ["openScannerNumOps","openScannerAvgTime","closeNumOps","closeAvgTime","nextNumOps","nextAvgTime","next.aboveOneSec.NumOps","next.aboveOneSec.AvgTime","putNumOps","putAvgTime","multiPutNumOps","multiPutAvgTime","multiPut.aboveOneSec.NumOps","multiPut.aboveOneSec.AvgTime","slowResponse.NumOps","slowResponse.AvgTime","getNumOps","getAvgTime","get.aboveOneSec.NumOps","get.aboveOneSec.AvgTime","deleteNumOps","deleteAvgTime","delete.aboveOneSec.NumOps","delete.aboveOneSec.AvgTime","bulkLoadHFileNumOps","bulkLoadHFileAvgTime","bulkLoadHFile.aboveOneSec.NumOps","bulkLoadHFile.aboveOneSec.AvgTime","RpcQueueTimeNumOps","RpcQueueTimeAvgTime","RpcProcessingTimeNumOps","RpcProcessingTimeAvgTime" ]
	}
